# Hume AI Credentials
# Get your API key from: https://platform.hume.ai/settings/keys
VITE_HUME_API_KEY=your_hume_api_key_here

# Anthropic API Key - Server-side (for Custom LLM endpoint)
# Get your API key from: https://console.anthropic.com/
# This is used by the Vercel serverless function - NOT exposed to browser
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Anthropic API Key - Client-side (for direct browser calls, development only)
# WARNING: This exposes the key in the browser - use server-side in production
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here
